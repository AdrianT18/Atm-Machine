<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="330.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.atm.ReportCard">
   <children>
      <Label layoutX="144.0" layoutY="14.0" prefHeight="39.0" prefWidth="312.0" text="Report a lost or stolen card here">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Label layoutX="44.0" layoutY="103.0" prefHeight="39.0" prefWidth="276.0" text="Please verify your account number">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="44.0" layoutY="161.0" prefHeight="39.0" prefWidth="235.0" text="Please verify your First Name ">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="VerifyAccount" layoutX="351.0" layoutY="110.0" />
      <TextField fx:id="VerifyName" layoutX="351.0" layoutY="168.0" />
      <Button fx:id="SubmitReport" layoutX="480.0" layoutY="283.0" mnemonicParsing="false" onAction="#reportCard" prefHeight="33.0" prefWidth="102.0" text="Submit" />
      <Button fx:id="BackReport" layoutX="354.0" layoutY="283.0" mnemonicParsing="false" onAction="#back" prefHeight="33.0" prefWidth="102.0" text="Back" />
      <Button fx:id="ExitReport" layoutX="44.0" layoutY="283.0" mnemonicParsing="false" onAction="#exit" prefHeight="33.0" prefWidth="102.0" text="Exit" />
   </children>
</AnchorPane>
